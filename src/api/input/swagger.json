{
  "openapi": "3.0.0",
  "paths": {
    "/collection/activities": {
      "get": {
        "operationId": "CollectionController_getAllCollectionActivities",
        "parameters": [
          {
            "name": "activities",
            "required": false,
            "in": "query",
            "example": [
              "TRANSFER",
              "SALE"
            ],
            "schema": {
              "enum": [
                "MINT",
                "BURN",
                "TRANSFER",
                "LIST",
                "DELIST",
                "SALE",
                "PATCH_ADDED",
                "PATCH_REMOVED"
              ],
              "type": "string"
            }
          },
          {
            "name": "sort",
            "required": false,
            "in": "query",
            "example": "DATE_DESC",
            "schema": {
              "enum": [
                "DATE_ASC",
                "DATE_DESC"
              ],
              "type": "string"
            }
          },
          {
            "name": "searchByCollectionNames",
            "required": true,
            "in": "query",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "page",
            "required": false,
            "in": "query",
            "example": 1,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "limit",
            "required": false,
            "in": "query",
            "example": 50,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "collectionNames",
            "required": false,
            "in": "query",
            "example": "flunks,backpack,inception-animals",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Get most recent activities",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/GetActivityDto"
                  }
                }
              }
            }
          }
        },
        "tags": [
          "collection"
        ]
      }
    },
    "/collection/{collectionName}/status": {
