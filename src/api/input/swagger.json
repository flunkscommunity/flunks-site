          }
        },
        "tags": [
          "collection"
        ]
      }
    },
    "/collection/{collectionName}": {
      "get": {
        "operationId": "CollectionController_getallNftsInACollection",
        "parameters": [
          {
            "name": "page",
            "required": false,
            "in": "query",
            "example": 1,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "limit",
            "required": false,
            "in": "query",
            "example": 50,
            "schema": {
              "type": "number"
            }
          },
          {
            "name": "sort",
            "required": false,
            "in": "query",
            "example": "PRICE_ASC",
            "schema": {
              "enum": [
                "PRICE_ASC",
                "PRICE_DESC",
                "DATE_ASC",
                "DATE_DESC",
                "RANK_ASC",
                "RANK_DESC"
              ],
              "type": "string"
            }
          },
          {
            "name": "include",
            "required": false,
            "in": "query",
            "example": "LISTED",
            "schema": {
              "enum": [
                "LISTED",
                "NOT_LISTED",
                "ALL"
              ],
              "type": "string"
            }
          },
          {
            "name": "traits",
            "required": false,
            "in": "query",
            "description": "Strategy: 'AND' when trait names are the same; 'OR' when trait names are different",
            "example": "{\"Head\": [\"Pink Bandana\", \"Blue  Headphones\"], \"Pigment\": [\"Purple\"]}",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "collectionName",
            "required": true,
            "in": "path",
            "example": "flunks",
            "schema": {}
          }
        ],
        "responses": {
          "200": {
            "description": "Get all NFTs by collectionName",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/MarketplaceIndividualNftDto"
                  }
                }
              }
            }
          }
        },
        "tags": [
          "collection"
        ]
      }
    },
